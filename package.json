{
  "name": "bitaunty",
  "version": "1.0.0",
  "description": "A crypto watchlist desktop application made with electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/btc.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/btc.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"CryptoApp\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/btc.ico --prune=true --out=release-builds"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navi25/BitAunty.git"
  },
  "keywords": [
    "Cryptocurrency",
    "Electron",
    "React",
    "DesktopApp"
  ],
  "author": "Navendra Jha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/navi25/BitAunty/issues"
  },
  "homepage": "https://github.com/navi25/BitAunty#readme",
  "devDependencies": {
    "electron": "3.0.11",
    "electron-packager": "^13.0.0",
    "electron-reload": "^1.4.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "path": "^0.12.7",
    "url": "^0.11.0"
  }
}
